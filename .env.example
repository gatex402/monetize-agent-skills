# Your agent's private key (KEEP THIS SECURE)
AGENT_PRIVATE_KEY=your_private_key_here

# Network for provisioning (CAIP-2). Optional; defaults to Base Mainnet.
# Base Mainnet = eip155:8453, Solana Mainnet = solana:5eykt4UsFv8P8NJdTREpY1vzqAQZSSfL
PROVISION_NETWORK=eip155:8453

# Origin URL of the API you are monetizing (your backend's base URL, not api.gatex402.dev). Defaults to https://example.com in the example.
ORIGIN_URL=https://your-api.example.com


# The management token received after provisioning
GATE_X402_MGMT_TOKEN=your_mgmt_token_here
